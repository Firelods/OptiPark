{
  "name": "mqtt-source-parking",
  "config": {
    "connector.class": "io.streamthoughts.kafka.connect.mqtt.MqttSourceConnector",
    "tasks.max": "1",
    "mqtt.server.uri": "tcp://mosquitto:1883",
    "mqtt.topics": "parking/+/status",
    "kafka.topic": "parking.events",
    "mqtt.client.id": "mqtt-source-connector",
    "mqtt.qos": "1",
    "converters.schemas": "false"
  }
}
